# Cursor Context Ignore File
# Files and directories to exclude from Cursor's context and indexing

# Node modules and dependencies
node_modules/
.npm/
.yarn/
.pnpm-store/

# Build outputs
dist/
build/
out/
.next/
.nuxt/
.vuepress/dist/
storybook-static/

# Cache directories
.cache/
.parcel-cache/
.vite/
.turbo/
.eslintcache
.stylelintcache

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage and test outputs
coverage/
.nyc_output/
.coverage/
junit.xml
test-results/

# Environment files (may contain sensitive data)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# IDE and editor files
.vscode/settings.json
.vscode/launch.json
.vscode/extensions.json
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Temporary files
*.tmp
*.temp
.tmp/
.temp/

# Lock files (can be large and not useful for context)
package-lock.json
yarn.lock
pnpm-lock.yaml

# Generated files that don't need context
*.d.ts.map
*.js.map
*.css.map

# Large media files
*.mov
*.mp4
*.avi
*.wmv
*.flv
*.webm
*.mkv

# Archives
*.zip
*.tar.gz
*.tgz
*.rar
*.7z

# Documentation that's not relevant for development
CHANGELOG.md
CONTRIBUTING.md
CODE_OF_CONDUCT.md

# Git files
.git/
.gitignore

# Figma plugin build outputs (these are generated)
scripts/*/figma-plugin-*/code.js

# Large JSON files that are generated
scripts/tokens/tokens.json
scripts/icons/icons.json
scripts/component-metadata/components.json

# Storybook build artifacts
.storybook/public/
chromatic-output/
